{
  "name": "muralpay-challenge",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "node scripts/clean-cache.mjs && next dev -p 3000",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "lint": "next lint --dir src",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx}\" --max-warnings=10",
    "prettier:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format": "prettier --write .",
    "fix-all": "npm run format && npm run lint:fix",
    "type-check": "tsc --noEmit",
    "test": "jest --detectOpenHandles --forceExit ",
    "test:watch": "jest --watch",
    "prepare": "husky",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@hookform/resolvers": "^4.0.0",
    "@next/bundle-analyzer": "^15.1.7",
    "@types/js-cookie": "^3.0.6",
    "@types/lint-staged": "^13.3.0",
    "@types/react-input-mask": "^3.0.6",
    "axios": "^1.7.9",
    "critters": "^0.0.25",
    "js-cookie": "^3.0.5",
    "moment": "^2.30.1",
    "next": "15.1.7",
    "react": "^18.0.0",
    "react-currency-input-field": "^3.9.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.54.2",
    "react-icons": "^5.4.0",
    "react-input-mask-next": "^3.0.0-alpha.12",
    "react-spinners": "^0.15.0",
    "react-toastify": "^11.0.3",
    "zod": "^3.24.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@emotion/jest": "^11.13.0",
    "@emotion/server": "^11.11.0",
    "@eslint/eslintrc": "^3",
    "@swc/core": "^1.10.18",
    "@swc/jest": "^0.2.37",
    "@swc/plugin-emotion": "^9.0.0",
    "@testing-library/dom": "^9.3.3",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "prettier": "^3.5.0",
    "sharp": "^0.33.5",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "lint-staged": {
    "**/*": [
      "npm run lint",
      "npm run format"
    ]
  }
}